<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd" id="WebApp_ID" version="3.0">

  <display-name>Archetype Created Web Application</display-name>
    <welcome-file-list>
        <welcome-file>user.jsp</welcome-file>
        <welcome-file>home.jsp</welcome-file>
        <welcome-file>captchaCheck.jsp</welcome-file>
        <welcome-file>captchaQuery.jsp</welcome-file>
    </welcome-file-list>
    <servlet>
        <servlet-name>Main</servlet-name>
        <servlet-class>com.web.servlet.MainServlet</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>Main</servlet-name>
        <url-pattern>/</url-pattern>
    </servlet-mapping>

    <listener>
        <listener-class>com.web.listener.ContextListener</listener-class>
    </listener>

</web-app>
